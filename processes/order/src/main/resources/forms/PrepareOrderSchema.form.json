{
  "schema": {
    "type": "object",
    "properties": {
      "deliveryDate": {
        "type": "string",
        "format": "date"
      },
      "itemCheckList": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "item": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "string"
                },
                "image": {
                  "type": "string"
                },
                "name": {
                  "type": "string"
                },
                "price": {
                  "type": "number"
                },
                "quantity": {
                  "type": "integer"
                }
              }
            }
          }
        }
      }
    },
    "$schema": "http://json-schema.org/draft-07/schema#"
  },
  "uischema": {
    "type": "VerticalLayout",
    "elements": [
      {
        "type": "Control",
        "scope": "#/properties/deliveryDate"
      },
      {
        "type": "Control",
        "scope": "#/properties/itemCheckList"
      }
    ]
  }
}