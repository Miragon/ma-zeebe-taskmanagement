    },
    "itemCheckList" : {
      "type" : "array",
      "items" : {
        "type" : "object",
        "properties" : {
          "item" : {
            "type" : "object",
            "properties" : {
              "id" : {
                "type" : "string"
              },
              "image" : {
                "type" : "string"
              },
              "name" : {
                "type" : "string"
              },
              "price" : {
                "type" : "number"
              },
              "quantity" : {
