# Camunda Platform #
## Image versions ##
CAMUNDA_PLATFORM_VERSION=8.4.0
ELASTIC_VERSION=8.8.2

## Configuration ##
ZEEBE_AUTHENTICATION_MODE=none
ZEEBE_CLIENT_ID=zeebe
ZEEBE_CLIENT_SECRET=zecret

# Set to 'true' to enable resource based authorizations for users and groups
RESOURCE_AUTHORIZATIONS_ENABLED=false

# Taskmanager #
## Database ##
TM_DATASOURCE_DB=taskmanager
TM_DATASOURCE_DRIVER=org.postgresql.Driver
TM_JDBC_BASE_URL=jdbc:postgresql://postgres:5432/${LOCAL_DATASOURCE_DB_NAME}
TM_DATASOURCE_USER=postgres
TM_DATASOURCE_PASSWORD=postgres